export EDITOR="nvim"
export ANDROID_HOME=/Users/$USER/Library/Android/sdk
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8
set WORK /Volumes/xen/Work

set PATH $PATH ~/Documents/worker/dotfiles/scripts/*
set PATH $PATH /usr/local/sbin
set PATH $PATH $ANDROID_HOME/tools
set PATH $PATH $ANDROID_HOME/platform-tools
set PATH $PATH $HOME/Documents/Flutter/flutter/bin
set PATH $PATH $HOME/.yarn/bin $HOME/.config/yarn/global/node_modules/.bin
set PATH $PATH $HOME/go/bin

export GOPATH=$HOME/Documents/programming/Go
export GOBIN=$HOME/Documents/programming/Go/bin
set PATH $PATH $HOME/Documents/programming/Go/bin
